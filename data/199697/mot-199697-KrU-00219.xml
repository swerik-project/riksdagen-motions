      <pb facs="riksdagen-motions-pdf/data/199697/mot_199697_KR_0219/mot_199697_KR_0219-0001.pdf"/>
      <div xml:id="i-WTfbU7dwmcJeSwYcAwsCNn" type="motHeader">
        <div xml:id="i-5uDAt7zPcvwB6EseSTYwjf" type="motTitle"/>
        <div xml:id="i-TVjpaQG3hTzzChqt5aRovz" type="motSubmissionInfo"/>
