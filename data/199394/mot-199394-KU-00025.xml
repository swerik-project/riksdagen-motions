      <pb facs="riksdagen-motions-pdf/data/199394/mot_199394_KU_25/mot_199394_KU_25-001.pdf"/>
      <div xml:id="i-Vu2cP1rSCyWtLmwPsZsHnH" type="motHeader">
        <div xml:id="i-MefDfvvPfmJJKMFHsPbyfh" type="motTitle"/>
        <div xml:id="i-U7teaqYfPZBvwzK4QA1Jhj" type="motSubmissionInfo"/>
