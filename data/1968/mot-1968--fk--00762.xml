      <fw xml:id="i-XToPmbruxYFHHYLGeYxXbR">
        Motioner i Första kammaren, nr 761—762 år 1968 3
      </fw>
      <div xml:id="i-DJodi6rN6Ay8dv647APwdV" type="motHeader">
        <div xml:id="i-BWLc1yTq2Bx3R1BcjkJr9g" type="motTitle"/>
