      <pb facs="riksdagen-motions-pdf/data/199798/mot_199798_N_0014/mot_199798_N_0014-0001.pdf"/>
      <div type="motHeader" xml:id="i-FoDpVyon4hGQfbFPhVnCXE">
        <div type="motTitle" xml:id="i-GowXXzhDJGuiNX7yBj2FfL"/>
        <div type="motSubmissionInfo" xml:id="i-6GnT2ckBZSoWp36uAKwYgy"/>
