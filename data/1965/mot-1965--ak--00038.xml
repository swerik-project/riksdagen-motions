      <fw xml:id="i-TvdRrwAokNmT4BSdk66eT1">
        Motioner i Andra kammaren, nr 37—39 år 1965 13
      </fw>
      <div xml:id="i-PWZjRya7HUUhKTGX6ZR19e" type="motHeader">
        <div xml:id="i-W4YB8cMHEA9eG1WZrCGu8B" type="motTitle"/>
