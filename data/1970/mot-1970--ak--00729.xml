      <fw xml:id="i-9F5qHjMCZgp6AfLnXfnMAf">
        16 Motioner i Andra kammaren, nr 728—730 år 1970
      </fw>
      <div type="motHeader" xml:id="i-SQN71J3rZZyvhpqYzErwJA">
        <div type="motTitle" xml:id="i-JjV4eQuBkxBDyM4vUxahrq"/>
        <div type="motSubmissionInfo" xml:id="i-SNomUY2aBnKFowdJRtAqxU"/>
