      <fw xml:id="i-WHWhok9BoVhh2gdV4yk9b3">
        Motioner i Andra kammaren, nr 495—496 år 1961 11
      </fw>
      <div xml:id="i-P3N4JFPDoTGWRL3pbtesnh" type="motHeader">
        <div xml:id="i-FNzZ1fkS3srohw9WfXVMZm" type="motTitle"/>
