      <fw xml:id="i-YGTM8jp3kYPahFaSSWJtgk">
        4 Motioner i Andra kammaren, nr 1308—1310 år 1970
      </fw>
      <div xml:id="i-WkLFX4DmFwM93kyc6eZTqR" type="motHeader">
        <div xml:id="i-DHBVV8phwGaPSzHLZU6CNL" type="motTitle"/>
