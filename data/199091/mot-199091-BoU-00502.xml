      <pb facs="riksdagen-motions-pdf/data/199091/mot_199091_BoU_502/mot_199091_BoU_502-001.pdf"/>
      <div xml:id="i-28FSQ9qNDmS7bYizrjdtC4" type="motHeader">
        <div xml:id="i-1QKY6y8785eYT43Hb4AoJ" type="motTitle"/>
