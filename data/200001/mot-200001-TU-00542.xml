      <pb facs="riksdagen-motions-pdf/data/200001/mot_200001_T_0542/mot_200001_T_0542-0001.pdf"/>
      <div type="motHeader" xml:id="i-LQFmmCT98GWy8ywvtjxyXr">
        <div type="motTitle" xml:id="i-HVRAdbM8f8vv8G7dVzYNsx"/>
        <div type="motSubmissionInfo" xml:id="i-D7GRVzSh7Qs8YyFvS6DweD"/>
