      <fw xml:id="i-VdGhvJmGzbYrdfNNXGmLQJ">
        Motioner i Första kammaren, nr 1143—1114,4 år 1970 3
      </fw>
      <div xml:id="i-BMEioGzQ6yS1tuC4H7eDfz" type="motHeader">
        <div xml:id="i-6rMukyCtLgwxpQzcFjWrvv" type="motTitle"/>
