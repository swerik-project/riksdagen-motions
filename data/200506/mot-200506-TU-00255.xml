      <pb facs="riksdagen-motions-pdf/data/200506/mot_200506_T_255/mot_200506_T_255-0001.pdf"/>
      <div xml:id="i-FWaPgks2qnevj7dKmSGaHH" type="motHeader">
        <div xml:id="i-HG4TBm3jQpDSraY463Eaq1" type="motTitle"/>
