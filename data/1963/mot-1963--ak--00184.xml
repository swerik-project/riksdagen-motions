      <fw xml:id="i-NJwYvwhMSWesP2rkdqzAed">
        Motioner i Andra kammaren, nr 182—184 år 1963 11
      </fw>
      <div xml:id="i-GGBEvuv7BCMdPBKXMC2FUz" type="motHeader">
        <div xml:id="i-7RH3QEdrG6ZKSATiDhtLw6" type="motTitle"/>
