      <pb facs="riksdagen-motions-pdf/data/199798/mot_199798_JU_0007/mot_199798_JU_0007-0001.pdf"/>
      <div xml:id="i-TAQ9CRen4rnurSc3mWx8XJ" type="motHeader">
        <div xml:id="i-LfnAjZReAepphFmP8z7Typ" type="motTitle"/>
