      <pb facs="riksdagen-motions-pdf/data/199293/mot_199293_KU_31/mot_199293_KU_31-001.pdf"/>
      <div xml:id="i-M93N5rXrzavgp1UkZwxLjG" type="motHeader">
        <div xml:id="i-MkSG1oqLx19ZaqWFMpqr2r" type="motTitle"/>
