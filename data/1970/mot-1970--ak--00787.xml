      <fw xml:id="i-GRjfURNtoUVcQtpK2wLJR8">
        8 Motioner i Andra kammaren, nr 786—787 år 1970
      </fw>
      <div xml:id="i-YHSHoRKxTKsWpZXGcGzQmF" type="motHeader">
        <div xml:id="i-3MYQTWvhLuQN8ukDmtFomU" type="motTitle"/>
