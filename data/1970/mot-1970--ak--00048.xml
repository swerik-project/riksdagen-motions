      <fw xml:id="i-Xw4Zk774fjbv4BndyEgwV3">
        Motioner i Andra kammaren, nr 47—48 år 1970 9
      </fw>
      <div xml:id="i-7E2uu4tAYbec1L8hsADzPr" type="motHeader">
        <div xml:id="i-PGjNNtTjhcq2SmxyBgfJRT" type="motTitle"/>
        <div xml:id="i-P7mHMdmWQQ7GE876KeU3jV" type="motSubmissionInfo"/>
