      <pb facs="riksdagen-motions-pdf/data/200102/mot_200102_A_0208/mot_200102_A_0208-001.pdf"/>
      <div type="motHeader" xml:id="i-FJnwf1GuYYxFpadvV5fuSd">
        <div type="motTitle" xml:id="i-FfT2LsKZ1sh7WVASXbWzpc"/>
        <div type="motSubmissionInfo" xml:id="i-A5U2aho6VfSQ6H1nZfMQqU"/>
