      <fw xml:id="i-HqC2HUyEkT4kqZRaZtU3si">
        Motioner i Andra kammaren, nr 964—966 år 1966 15
      </fw>
      <div xml:id="i-HgctVPc1PX1ZS9tSsXy8NK" type="motHeader">
        <div xml:id="i-8E3HvtLDosqJbZpYNCuC1t" type="motTitle"/>
