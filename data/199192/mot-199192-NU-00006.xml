      <pb facs="riksdagen-motions-pdf/data/199192/mot_199192_NU_6/mot_199192_NU_6-001.pdf"/>
      <div xml:id="i-RtuXAppRk7oADBqXhv1Vos" type="motHeader">
        <div xml:id="i-UM2dZj2hZDAVipMCbsEoH8" type="motTitle"/>
