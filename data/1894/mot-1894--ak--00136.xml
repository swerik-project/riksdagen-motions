      <pb facs="riksdagen-motions-pdf/data/1894/mot_1894__ak__136/mot_1894__ak__136-001.pdf"/>
      <fw xml:id="i-RuyurWArQZYUJbq9b5MBuc">
        Motioner i Andra Kammaren, N:o 136. 3
      </fw>
      <div xml:id="i-Kn5F3hprLnyvjiAxahV8TH" type="motHeader">
        <div xml:id="i-FxfXcvYxySQ4as8PKzcfGJ" type="motTitle"/>
        <div xml:id="i-2u9NqP1HnE8D7CHg1ddLtW" type="motSubmissionInfo"/>
      </div>
      <div xml:id="i-JjNw3hqMqqCzJCEVGM55e6" type="motBody">
        <div xml:id="i-JX9yxPAufP5vdiG35cFqVT">
          <p xml:id="i-Aq355AcEAfdrNJRNhW6mKt">
            N:o 136.
          </p>
          <p xml:id="i-LzY2QY2ZHgQaNnoiM4tnX7">
            Af herr E. Hammarlund, med förslag angående tillägg till g 25
            mom. 3 t förordningen angående kyrkostämma samt kyrko- råd och
            skolråd i
          </p>
          <p xml:id="i-2hH5ZC4zzpd2MAEtuSrWbq" type="date">
            Stockholm den 20 november 1863.
          </p>
          <div xml:id="i-2CbgzMChcngMdZcyJkSyD8" type="signatureBlock">
            <list xml:id="i-7f3iS1ppy6ho8ytDL3Z3Nq">
              <item xml:id="i-61Reae17dMkpw8TGQqfRU6" who="unknown" type="signature">
                Då några giltiga skäl ej föreligga för en undantagslagstiftning
                för Stockholms stad beträffande val af vice ordförande i skolrådet
              </item>
              <item xml:id="i-W8gHZwSjYMDwqHsR1c9Vbu" who="unknown" type="signature">
                får jag — med hänvisning till min denna dag afgifna motion N:o
                135 -— hemställa
              </item>
            </list>
          </div>
          <p xml:id="i-ExPgAgVRYUMSrPe9YwP8xy">
            att Riksdagen måtte besluta, att närmast efter mom. 3 af 8 25
            i förordningen om kyrkostämma samt kyrko- råd och skolråd i
          </p>
          <p xml:id="i-GUh6JSznCbvtmFgW9u1kvA" type="date">
            Stockholm den 20 november 1863
          </p>
          <div xml:id="i-2wBve1n3ywxEUh5n9FpMpS" type="signatureBlock">
            <list xml:id="i-DrwUhYzpURFS3fXYxg1RA8">
              <item xml:id="i-KSWzXtLGm3XPsaj8MYSwap" who="unknown" type="signature">
                måtte insättas ett nytt moment af följande lydelse: »Skolrådet
                väljer inom sig
              </item>
              <item xml:id="i-8oeGFB7d1KefDu4Du3HcR1" who="unknown" type="signature">
                särskildt för hvarje år
              </item>
              <item xml:id="i-6KMnewF9fwoskaELf73tYM" who="unknown" type="signature">
                en vice ordförande
              </item>
            </list>
          </div>
          <p xml:id="i-SJ3vR7GMYJbcXMjbqc6Zu1" type="date">
            Stockholm den 28 januari 1894.
          </p>
          <div xml:id="i-CqrYULGmjuGQKfaxYYNZSn" type="signatureBlock">
            <list xml:id="i-PoinRE973wjumqDAJ1SPA7">
              <item xml:id="i-RQuibJXLyY9qdumaZ2V6kr" who="unknown" type="signature">
                Emil Hammarhimd
              </item>
            </list>
          </div>
        </div>
      </div>
