      <fw xml:id="i-EMC5Ak6NuEr7J3qx5YEJGH">
        Motioner i Första kammaren, nr 941 — 942 år 1970 3
      </fw>
      <div xml:id="i-Lz6Ln6U8MWWAKnLR1aGuiY" type="motHeader">
        <div xml:id="i-NfETeAR1s3mFHpcfvgKGuC" type="motTitle"/>
