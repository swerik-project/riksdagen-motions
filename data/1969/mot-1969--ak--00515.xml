      <fw xml:id="i-9XnrLsP976cgbcNApJAtex">
        6 Motioner i Andra kammaren, nr 514—515 år 1969
      </fw>
      <div xml:id="i-TCu2qJgx1FBxXhqFTpknE7" type="motHeader">
        <div xml:id="i-GFgAHTjLuKfBgw2dVF3m5s" type="motTitle"/>
