      <pb facs="riksdagen-motions-pdf/data/199900/mot_199900_K_0300/mot_199900_K_0300-0001.pdf"/>
      <div type="motHeader" xml:id="i-X3cxBBYhq3CuwxMHMDNM7m">
        <div type="motTitle" xml:id="i-StzU8JsZKLmNF4obxJcSSi"/>
        <div type="motSubmissionInfo" xml:id="i-X6ERspicVT2PdepHJeXMw6"/>
