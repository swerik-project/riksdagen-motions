      <fw xml:id="i-APDi1cADWasqAuLYN5Yzg">
        4 Motioner i Andra kammaren, nr 1479—1480 år 1970
      </fw>
      <div xml:id="i-D9TkjVQyGXkkB6j6KHmLX3" type="motHeader">
        <div xml:id="i-4mQmeW5Lj8c6Zp5aSaRnSz" type="motTitle"/>
