      <fw xml:id="i-Cwq4j3uaaBpZtJBfhmRP1q">
        Motioner i Andra kammaren, nr 920—921 år 1968
      </fw>
      <div xml:id="i-YSKuL7s5EmiURyVuhfPY1F" type="motHeader">
        <div xml:id="i-QYkvxWif3yVkMyNxFcLAFm" type="motTitle"/>
