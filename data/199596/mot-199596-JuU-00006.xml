      <pb facs="riksdagen-motions-pdf/data/199596/mot_199596_JU_0006/mot_199596_JU_0006-0001.pdf"/>
      <div type="motHeader" xml:id="i-MnUjZTyRaUmmbkkakrSusC">
        <div type="motTitle" xml:id="i-GNe1rAfb1pWAwNgcqKaHHF"/>
        <div type="motSubmissionInfo" xml:id="i-2iSkACud4xea8pW481JQ1Z"/>
