      <pb facs="riksdagen-motions-pdf/data/199495/mot_199495_BoU_529/mot_199495_BoU_529-001.pdf"/>
      <div xml:id="i-HFo3nDNDgqAnA4Tc2UVat1" type="motHeader">
        <div xml:id="i-Tp3ZgxW2xt5aoW8fD1hb49" type="motTitle"/>
        <div xml:id="i-EXJvqmSDZnmDQoFSJBGwZr" type="motSubmissionInfo"/>
