      <fw xml:id="i-JtR8m6bD1kr3hJ5Ym12WN8">
        16 Motioner i Första kammaren, nr 187 år 1958
      </fw>
      <div xml:id="i-WTkDgKnZw3KsZGfyRaqedR" type="motHeader">
        <div xml:id="i-U11yGgJp9xUkmUx7QNoJLU" type="motTitle"/>
