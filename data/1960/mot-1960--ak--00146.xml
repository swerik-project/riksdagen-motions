      <fw xml:id="i-TKkekbQfpxc5MxWJS3Wm57">
        Motioner i Andra kammaren, nr 145—146 år 1960
      </fw>
      <div xml:id="i-VApM6FdNdEJBG2Y6SsSzBD" type="motHeader">
        <div xml:id="i-52TL1qbmScPBuCzATyohRn" type="motTitle"/>
        <div xml:id="i-42hWp7HTSooc1Gg44bjguL" type="motSubmissionInfo"/>
