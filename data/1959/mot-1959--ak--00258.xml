      <fw xml:id="i-XUupzsv9qgrFD3dE3pktYi">
        16 Motioner i Andra kammaren, nr 257—258 år 1959
      </fw>
      <div xml:id="i-DkzpzJZTah56wHFLoKwqSh" type="motHeader">
        <div xml:id="i-4ALRmB7N5dLeZiJjYSJ4ku" type="motTitle"/>
