      <fw xml:id="i-WyJyLhgzdWaK6TdHkkDe4i">
        Motioner i Andra kammaren, nr 764—765 år 1964 13
      </fw>
      <div xml:id="i-X79gXHXXJJvXhdTdxip8aN" type="motHeader">
        <div xml:id="i-9tNCoweo5uVoi9PbrHh5Yn" type="motTitle"/>
        <div xml:id="i-JoWrdCykW8CCvj4RQbzDZj" type="motSubmissionInfo"/>
