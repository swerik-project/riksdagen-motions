      <fw xml:id="i-J5NBmJj7dJVLathpwBe3Gs">
        16 Motioner i Andra kammaren, nr 618-619 år 1970
      </fw>
      <div xml:id="i-AcqZYvoLS7UB9gnoDHwLge" type="motHeader">
        <div xml:id="i-Dgc4rjcjvzWy4ehag7P2PQ" type="motTitle"/>
