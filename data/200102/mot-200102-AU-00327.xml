      <pb facs="riksdagen-motions-pdf/data/200102/mot_200102_A_0327/mot_200102_A_0327-0001.pdf"/>
      <pb facs="riksdagen-motions-pdf/data/200102/mot_200102_A_0327/mot_200102_A_0327-001.pdf"/>
      <div xml:id="i-GVuJ3zK7KS1L66W4t3PsWo" type="motHeader">
        <div xml:id="i-Sj8Kk2cAtRxbQcW9BZ8cKy" type="motTitle"/>
