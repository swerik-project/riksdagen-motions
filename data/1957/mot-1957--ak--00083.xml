      <fw xml:id="i-QL2od64W2ufMEgEazDNxUD">
        14 Motioner i Andra kammaren, nr 82-—83 år 1957
      </fw>
      <div xml:id="i-5hL26z3W1XeCHMC9nsr5BB" type="motHeader">
        <div xml:id="i-8srJynKov3yHJ9eCPUo83Q" type="motTitle"/>
