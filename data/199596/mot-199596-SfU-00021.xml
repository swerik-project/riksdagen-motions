      <pb facs="riksdagen-motions-pdf/data/199596/mot_199596_SF_0021/mot_199596_SF_0021-0001.pdf"/>
      <div type="motHeader" xml:id="i-STwGkVgZwQD7ocCuyKQfHg">
        <div type="motTitle" xml:id="i-31J9jbcJx8hCUcqHskR1VF"/>
        <div type="motSubmissionInfo" xml:id="i-Ary6G5Qe21pcSHFvuRAye5"/>
