      <pb facs="riksdagen-motions-pdf/data/199091/mot_199091_NU_13/mot_199091_NU_13-001-001.pdf"/>
      <div xml:id="i-XrGsJFP22cxm6JeYHBh2bd" type="motHeader">
        <div xml:id="i-76fNKHVcZrZgP2PLN9oU11" type="motTitle"/>
