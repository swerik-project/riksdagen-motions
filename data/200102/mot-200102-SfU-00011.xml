      <pb facs="riksdagen-motions-pdf/data/200102/mot_200102_Sf_0011/mot_200102_Sf_0011-0001.pdf"/>
      <div xml:id="i-MX5m5dk7DSNA9RGDc45Uz9" type="motHeader">
        <div xml:id="i-JiZb7yn7mf1G1qxxKWZToe" type="motTitle"/>
        <div xml:id="i-AV3LdhYmvcFRH5HR6tSRqN" type="motSubmissionInfo"/>
