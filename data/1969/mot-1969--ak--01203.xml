      <fw xml:id="i-MoeQvoDjfgUfJYvb38HtmG">
        Motioner i Andra kammaren, nr 1202—1203 år 1969 19
      </fw>
      <div xml:id="i-Wic7qjAdtNcP6iLBFtyWXX" type="motHeader">
        <div xml:id="i-W5ctT4Y7EgUExSyUkeQ8YK" type="motTitle"/>
        <div xml:id="i-74bPf9qqbdcCWTdGwNdefK" type="motSubmissionInfo"/>
