    <body>
      <pb facs="riksdagen-motions-pdf/data/200304/mot_200304_MJ_0329/mot_200304_MJ_0329-0001.pdf"/>
      <div xml:id="i-4E1JjZdwjSK7JQ79eWwtZN" type="motHeader">
        <div xml:id="i-GcjS98BeZZeLYDFxuycTgL" type="motTitle"/>
