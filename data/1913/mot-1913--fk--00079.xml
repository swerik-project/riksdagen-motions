      <pb facs="riksdagen-motions-pdf/data/1913/mot_1913__fk__79/mot_1913__fk__79-001.pdf"/>
      <div xml:id="i-Jz7GtFWw1RSrgWaneJY1Wu" type="motHeader">
        <div xml:id="i-KA4H61giML7cPW1CQ887CE" type="motTitle"/>
        <div xml:id="i-KdNgd9LUfGHgA6Xojhfpap" type="motSubmissionInfo"/>
