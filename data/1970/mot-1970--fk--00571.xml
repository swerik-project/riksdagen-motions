      <fw xml:id="i-HLWLZVhdcubUdfE5RrQ3AF">
        10 Motioner i Första kammaren, nr 569—571 år 1970
      </fw>
      <div type="motHeader" xml:id="i-RMwVAyGpXJ8iFNDSnDRjun">
        <div type="motTitle" xml:id="i-5JZgvEEufMG4xUwLLEbbSY"/>
        <div type="motSubmissionInfo" xml:id="i-uDUacRLBkQVRZWGEAEtJQ"/>
