      <fw xml:id="i-KemVWYD8m1hHE7TPL1pxRa">
        Motioner i Andra kammaren, nr 399—400 år 1969 5
      </fw>
      <div xml:id="i-5qaaSStmmJ3w7J9uGNm56R" type="motHeader">
        <div xml:id="i-GuERvGcdEDNvtygNNii1KH" type="motTitle"/>
