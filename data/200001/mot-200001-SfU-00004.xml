    <body>
      <pb facs="riksdagen-motions-pdf/data/200001/mot_200001_Sf_0004/mot_200001_Sf_0004-0001.pdf"/>
      <div type="motHeader" xml:id="i-VPxYNtRRtjgdMhFmHMWq5M">
        <div type="motTitle" xml:id="i-JQUodoDJWfPPiVFCGwb6bN"/>
        <div type="motSubmissionInfo" xml:id="i-2kcSWj1TNRQtKxq21vVC5C"/>
