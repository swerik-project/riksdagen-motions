      <pb facs="riksdagen-motions-pdf/data/199192/mot_199192_SfU_41/mot_199192_SfU_41-001-001.pdf"/>
      <div xml:id="i-YMY7pKqL1nyHGXgaSBfh6T" type="motHeader">
        <div xml:id="i-DJa9j3K6JX3grXpXBe9axi" type="motTitle"/>
