      <pb facs="riksdagen-motions-pdf/data/199798/mot_199798_SF_0043/mot_199798_SF_0043-0001.pdf"/>
      <div xml:id="i-MWshnknoKS1X1yUgzHeNEk" type="motHeader">
        <div xml:id="i-W9F8vdfif5Yq28ydqCzmdP" type="motTitle"/>
        <div xml:id="i-QLrhJPrK58xZdMVJjRLhyb" type="motSubmissionInfo"/>
